Se integro clase Tienda con inventario, proveedores y ventas
package com.mycompany.proyecto_super_ahorro;
import java.util.ArrayList;
import java.util.List;

public class Tienda {
    private Inventario inventario = new Inventario();
    private List<Proveedor> proveedores = new ArrayList<>();
    private List<Venta> ventas = new ArrayList<>();

    public void registrarProveedor(Proveedor p) { proveedores.add(p); }
    public void agregarProducto(Producto p) { inventario.agregarProducto(p); }
    public void registrarVenta(Venta v) { ventas.add(v); }
}
