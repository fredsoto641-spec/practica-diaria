Se implemento ColaPedidos y PilaVentas
// ColaPedidos.java
package com.mycompany.proyecto_super_ahorro;
import java.util.LinkedList;
import java.util.Queue;

public class ColaPedidos {
    private Queue<String> pedidos = new LinkedList<>();
    public void agregarPedido(String pedido) { pedidos.add(pedido); }
    public String procesarPedido() { return pedidos.poll(); }
}
