Se creo clase Venta y clase interna ItemVenta
package com.mycompany.proyecto_super_ahorro;
import java.util.ArrayList;
import java.util.List;

public class Venta {
    private List<ItemVenta> items = new ArrayList<>();

    public void agregarItem(Producto producto, int cantidad) {
        items.add(new ItemVenta(producto, cantidad));
    }

    public double total() {
        double suma = 0;
        for (ItemVenta i : items) {
            suma += i.producto.getPrecio() * i.cantidad;
        }
        return suma;
    }

    class ItemVenta {
        Producto producto;
        int cantidad;

        ItemVenta(Producto p, int c) {
            this.producto = p;
            this.cantidad = c;
        }
    }
}

