Se a√±adio clase GestionProductos para control general
package com.mycompany.proyecto_super_ahorro;

public class GestionProductos {
    private Tienda tienda = new Tienda();

    public void demo() {
        Producto p1 = new Producto("Arroz", 3.5, 50);
        Producto p2 = new Producto("Leche", 4.0, 30);
        tienda.agregarProducto(p1);
        tienda.agregarProducto(p2);

        tienda.inventario.listarProductos();
    }
}
